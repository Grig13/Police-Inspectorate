@model Case
@{
    ViewData["Title"] = "Add Cases";
}
<h1 class="m-3">Add new Cases</h1>

<form method="post">
    <div class="mb-3">
        <label asp-for="BeginDate"></label>
        <input type="date" asp-for="BeginDate" class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="EndDate"></label>
        <input type="date" asp-for="EndDate" class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="Status"></label>
        <input asp-for="Status" class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="Description"></label>
        <input asp-for="Description" class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="Agents"></label>
        <input asp-for="Agents" class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="Suspects"></label>
        <input asp-for="Suspects" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary" style="width:150px">Create</button>
    <a asp-controller="News" asp-action="Index" class="btn btn-primary" style="width:150px">Back to List</a>
</form>